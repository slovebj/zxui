{
  "name": "zxui",
  "version": "0.0.1",
  "description": "A Component Library for Vue.js.",
  "main": "lib/zxui.common.js",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "start": "cooking watch -c build/cooking.markcook.js -p",
    "new": "node build/bin/new.js",
    "zxui:c": "node-sass src-css/SCSS/basecss/zx-components.scss src-css/SCSS/basecss/zx-components.css",
    "zxui": "node-sass src-css/SCSS/basecss/zxbase.scss > src-css/SCSS/basecss/zxbase.css",
    "markcook": "cooking build -c build/cooking.markcook.js -p",
    "md2html": "node ajax/demo/md2html.js",
    "demo": "node ajax/demo/demo.js",
    "build:entry": "node build/bin/build-entry.js",
    "dev": "npm run bootstrap && npm run build:entry",
    "dist": "npm run build:entry && npm run lint && cooking build -c build/cooking.conf.js,build/cooking.component.js -p && npm run build:utils",
    "dist:c": "npm run lint:c && cooking build -c build/cooking.conf.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "deploy": "npm run build:file && cooking build -c build/cooking.demo.js -p && echo element.eleme.io>>examples/zxui/CNAME && gh-pages -d examples/element-ui --remote eleme && del examples/element-ui",
    "bootstrap": "npm i && lerna bootstrap",
    "pub": "sh build/release.sh",
    "build:utils": "babel src/utils --out-dir lib/utils",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "lint": "eslint src/**/*.js packages/**/*.{js,vue} build/bin/*.js --quiet",
    "lint:c": "eslint packages/**/*.{js,vue} --quiet",
    "test:watch": "karma start test/unit/karma.conf.js",
    "test": "npm run lint && karma start test/unit/karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:elemefe/element.git"
  },
  "homepage": "http://element.eleme.io",
  "keywords": [
    "eleme",
    "vue",
    "components"
  ],
  "bugs": {
    "url": "https://github.com/elemefe/element/issues"
  },
  "dependencies": {
    "async-validator": "^1.6.6",
    "cheerio": "^0.22.0",
    "codemirror": "^5.21.0",
    "highlight.js": "^9.8.0",
    "markdown-it": "^8.1.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.3.0",
    "object-assign": "^4.1.0",
    "object-equal": "^1.0.0",
    "throttle-debounce": "^1.0.1",
    "vue-popup": "^0.2.9",
    "wind-dom": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "cooking": "^1.5.0",
    "cooking-lint": "^0.1.4",
    "cooking-vue2": "^0.3.1",
    "coveralls": "^2.11.14",
    "cp-cli": "^1.0.2",
    "css-loader": "^0.26.0",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "file-save": "^0.2.0",
    "gh-pages": "^0.11.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-postcss": "^6.1.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0-beta2",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "json-templater": "^1.0.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "lerna": "2.0.0-rc.4",
    "lolex": "^1.5.1",
    "mocha": "^3.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.1.1",
    "postcss-salad": "^1.0.5",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "theaterjs": "^3.0.0",
    "uppercamelcase": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue": "^2.3.2",
    "vue-loader": "^12.0.3",
    "vue-markdown-loader": "^0.6.2",
    "vue-template-compiler": "^2.3.2",
    "vuex": "^2.3.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12"
  }
}
